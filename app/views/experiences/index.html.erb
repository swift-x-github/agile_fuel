<p id="notice"><%= notice %></p>

<h3>Experiences</h3>

<table class="table" >
  <thead>
    <tr>
      <th>Name</th>
       <th>Rating</th>
       <th></th>
       <th></th>
       <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @experiences.each do |experience| %>
      <tr>
        <td><%= experience.name %></td>
        <td><%= experience.rate %></td>
        <td><%= link_to 'Show', experience %></td>
        <td><%= link_to 'Edit', edit_experience_path(experience) %></td>
        <td><%= link_to 'Destroy', experience, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Experience', new_experience_path, class:  'btn btn-primary' %>
